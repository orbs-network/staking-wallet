{
  "name": "staking-wallet",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "homepage": "https://staking.orbs.network",
  "engines": {
    "node": "10.17.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest --config ./src/__tests__/jest.config.js",
    "test-watch": "jest --watch --config ./src/__tests__/jest.config.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "type-check": "tsc -p ./src/tsconfig.json",
    "build": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack -p",
    "dev": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -w",
    "dev-fork": "cross-env ETHEREUM_NETWORK=mainnet-fork NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -w",
    "dev-rops": "cross-env ETHEREUM_NETWORK=ropsten NODE_ENV=development TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -w",
    "dev-prod": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -w",
    "build-server": "tsc -p ./src/server",
    "deployOld": "rm -rf ./dist && npm run build && cross-env DEPLOY_REPO=staking DEPLOY_USER=orbs-network ./scripts/deploy.sh",
    "predeploy": "rm -rf ./dist && npm run build",
    "deploy": "gh-pages -d dist -r git@github.com:orbs-network/staking.git",
    "mkd": "mkdir -p ./src/local && mkdir -p ./ganache-env/_out",
    "postinstall": "npm run mkd",
    "sync-translations": "./src/translations/syncTranslations.zsh"
  },
  "keywords": [
    "typescript",
    "react",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/orbs-network/staking-wallet.git"
  },
  "license": "MIT",
  "author": "Gil Amran",
